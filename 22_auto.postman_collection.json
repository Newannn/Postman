		{
			"name": "Create User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.environment.set(\"email\", jsonData.email);",
							"pm.environment.set(\"userID\", jsonData.id);",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Проверка что пользователь с таким id создан\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"id\");",
							"});",
							"",
							"pm.test(\"Body firstName\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"firstName\");",
							"});",
							"",
							"",
							"pm.test(\"lastName\", function () {",
							"    pm.expect(pm.response.text()).to.include(\"lastName\");",
							"});",
							"",
							"pm.test(\"Проверка наличия символа \\\"@\\\" в поле email\", function () {",
							"    var email = pm.response.json().email;",
							"",
							"    pm.expect(email).to.include(\"@\");",
							"});",
							"",
							"pm.test(\"Отсутствие символов кириллицы в поле email\", function () {",
							"    var email = pm.response.json().email;",
							"",
							"    pm.expect(email).to.match(/^[^\\u0400-\\u04FF]+$/);",
							"});",
							"",
							"pm.test(\"email на отсутствие запрещенных специальных символов кроме (точки, тере, нижнего подчеркивания) . - _\", function () {",
							"    var email = pm.response.json().email;",
							"",
							"    pm.expect(email).to.match(/^[a-zA-Z0-9.-_]+$/);",
							"});",
							"",
							"// HEADERS",
							"",
							"pm.test(\"Заголовок Content-Type установлен\", function () {",
							"    pm.response.to.have.header(\"Content-Length\");",
							"});",
							"",
							"pm.test(\"Заголовок Access-Control-Allow-Origin\", function () {",
							"    pm.response.to.have.header(\"Access-Control-Allow-Origin\");",
							"});",
							"",
							"pm.test(\"Заголовок Cache-Control\", function () {",
							"    pm.response.to.have.header(\"Cache-Control\");",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
